#!/usr/bin/env bash
dir="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
prog=`basename $0`
. ${dir}/oc_profile 2>/dev/null
COMM="-logrota"
LOGA=">${dir}/../logs/logrota.$HOSTNAME.$USER.log"
if [[ "$#" -eq 0 ]]; then
	COMM=""
	LOGA=""
	echo "ERROR: missing domain directory to run"
fi
"$JAVA_HOME/bin/java" $JAVAEX -Dprog="$dir/$prog" -jar ${dir}/lib/OC.jar ${COMM} "$@" ${LOGA} 2>/dev/null
exit $?
